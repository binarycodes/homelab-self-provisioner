immich.cloudyhome.net {
    reverse_proxy media.ip.cloudyhome.net:2283
    tls {
        dns porkbun {
            api_key {{ porkbun_api_key }}
            api_secret_key {{ porkbun_secret_api_key }}
        }
    }
}

immich-frame.cloudyhome.net {
    reverse_proxy media.ip.cloudyhome.net:8080
    tls {
        dns porkbun {
            api_key {{ porkbun_api_key }}
            api_secret_key {{ porkbun_secret_api_key }}
        }
    }
}
